{
  "name": "jambagecom/import-tt-board-comments",
  "type": "typo3-cms-extension",
  "description": "Import of the comments table into tt_board",
  "keywords": [
    "TYPO3",
    "extension",
    "import"
  ],
  "support": {
    "issues": "https://github.com/franzholz/import/issues"
  },
  "homepage": "https://jambage.com",
  "authors": [
    {
      "name": "Franz Holzinger",
      "role": "Developer",
      "homepage": "https://ttproducts.de"
    }
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "typo3/cms-core": "^7.6 || ^8,7 || ^9.5",
    "jambagecom/import": "^0.5",
    "friendsoftypo3/func": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "JambageCom\\ImportTtBoardComments\\": "Classes"
    }
  },
  "replace": {
    "jambagecom/import-tt-board-comments": "self.version",
    "typo3-ter/import-tt-board-comments": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "import_tt_board_comments"
    }
  }
}
